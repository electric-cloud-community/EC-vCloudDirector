<project name="EC-vCloudDirector" default="package" basedir=".">
	<description>
        Build the EC-vCloudDirector plugin
    </description>

    <property name="buildconfigdir" value="../Plugin-In-A-Box/scripts" />
    
	<!-- plugin specific properties -->
	<property name="pluginKey" value="EC-vCloudDirector" />
	<property name="pluginVersion" value="1.0" />
	<property name="gwtModules" value="ecplugins.vclouddirector.ConfigurationManagement" />
    <property name="package.export.src" value="vclouddirector.srcs"/>

    <import file="${buildconfigdir}/buildTargets.xml"/>

    <!-- Package sources for exported utility classes. -->
    <fileset id="vclouddirector.srcs" dir="${dir.src}">
      <include name="**/PluginConstants.java"/>
      <include name="**/vCloudDirectorConfigList.java"/>
      <include name="**/vCloudDirectorConfigListLoader.*"/>
    </fileset>
</project>
